{
  "name": "embedcss",
  "version": "0.0.1",
  "main": "./js/index.js",
  "types": "./js/index.d.ts",
  "exports": {
    ".": {
      "types": "./js/index.d.ts",
      "import": "./js/index.js",
      "require": "./js/index.js"
    },
    "./binding": {
      "types": "./js/plugins/binding/index.d.mts",
      "import": "./js/plugins/binding/index.mjs",
      "require": "./js/plugins/binding/index.mjs"
    },
    "./plugins/esbuild": {
      "types": "./js/plugins/esbuild.d.mts",
      "import": "./js/plugins/esbuild.mjs",
      "require": "./js/plugins/esbuild.mjs"
    },
    "./plugins/vite": {
      "types": "./js/plugins/vite.d.mts",
      "import": "./js/plugins/vite.mjs",
      "require": "./js/plugins/vite.mjs"
    },
    "./plugins/webpack": {
      "types": "./js/plugins/vite.d.mts",
      "import": "./js/plugins/webpack.mjs",
      "require": "./js/plugins/webpack.mjs"
    }
  },
  "files": [
    "js"
  ],
  "scripts": {
    "build": "./build.sh && bun run build:types",
    "build:types": "tsc -p ./js/plugins"
  },
  "author": "Szymon Bretner (ncpa0cpl)",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.13.9",
    "dprint": "^0.49.1",
    "esbuild": "^0.25.0",
    "typescript": "^5.8.2",
    "vite": "^7.3.1"
  }
}
